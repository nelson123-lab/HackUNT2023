{
  "manifest_version": 3,
  "name": "SimpliFin",
  "description": "Learn Everything, Master Anything: Finance at your Fingertips",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png"
    }
  }
}